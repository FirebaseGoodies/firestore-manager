{
  "name": "firestore-manager",
  "version": "1.4.2",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng run firestore-manager:build-webextension --configuration=production",
    "build:firefox": "npm run build && cd dist/firestore-manager/webextension && mv -f manifest.firefox.json manifest.json",
    "build:chrome": "npm run build && cd dist/firestore-manager/webextension && rm -f manifest.firefox.json",
    "package": "web-ext build --source-dir=dist/firestore-manager/webextension --artifacts-dir=. --overwrite-dest",
    "build:github": "ng build --prod --base-href \"/firestore-manager/\"",
    "deploy:github": "ngh --dir=dist/firestore-manager/webapp",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^9.1.12",
    "@angular/common": "^9.1.12",
    "@angular/compiler": "^9.1.12",
    "@angular/core": "^9.1.12",
    "@angular/fire": "^6.0.2",
    "@angular/forms": "^9.1.12",
    "@angular/platform-browser": "^9.1.12",
    "@angular/platform-browser-dynamic": "^9.1.12",
    "@angular/router": "^9.1.12",
    "ang-jsoneditor": "^1.10.0",
    "firebase": "7.13.1",
    "jsoneditor": "^9.0.4",
    "ng-zorro-antd": "^8.5.2",
    "ngx-diff2html": "^0.1.3",
    "rxjs": "~6.5.4",
    "tslib": "^1.10.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.900.7",
    "@angular/cli": "^9.1.12",
    "@angular/compiler-cli": "^9.1.12",
    "@angular/language-service": "^9.1.12",
    "@types/jasmine": "~3.5.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^12.11.1",
    "codelyzer": "^5.1.2",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~6.3.14",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~2.1.0",
    "karma-jasmine": "~3.0.1",
    "karma-jasmine-html-reporter": "^1.4.2",
    "protractor": "~7.0.0",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.0",
    "typescript": "~3.8.3",
    "web-ext-types": "^3.2.1",
    "webextension-polyfill": "^0.6.0"
  }
}
